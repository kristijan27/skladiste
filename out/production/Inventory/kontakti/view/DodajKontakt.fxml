<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="376.0" styleClass="background" stylesheets="@dodaj_kontakt.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="kontakti.controller.DodajKontaktController">
    <children>
        <GridPane layoutX="1.0" layoutY="-3.0" prefHeight="400.0" prefWidth="600.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="289.0" minWidth="10.0" prefWidth="131.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="518.0" minWidth="10.0" prefWidth="469.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label prefHeight="25.0" prefWidth="265.0" text="DODAJ NOVI KONTAKT" GridPane.columnSpan="2">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
                <Label fx:id="ime" text="   IME" GridPane.rowIndex="1"/>
                <Label fx:id="email" text="   EMAIL" GridPane.rowIndex="2"/>
                <Label fx:id="pass" text="   Password" GridPane.rowIndex="3"/>
                <TextField fx:id="imeTxt" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="emailTxt" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField fx:id="passTxt" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <Button fx:id="closeButton" mnemonicParsing="false" onAction="#spasi" text="Dodaj kontakt" GridPane.columnIndex="1"
                        GridPane.rowIndex="4"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
